#!/usr/bin/env bash
# cp metafranquin65.svg A.svg

sed '/stroke:rgb(100.000000%,0.000000%,0.000000%)/d' metafranquin65.svg > A.svg
# sed '/fill: rgb(100.000000%,100.000000%,100.000000%)/d' A.svg > A-black.svg
# sed '/stroke:rgb(0.000000%,0.000000%,0.000000%)/d' A.svg > A-white.svg
sed -i -e 's/style="stroke:rgb(0.000000%,0.000000%,0.000000%)/id="black" style="stroke:rgb(0.000000%,0.000000%,0.000000%)/g' A.svg
#
#
inkscape --select=black \
          --verb SelectionUnGroup \
          --verb StrokeToPath \
          --verb SelectionUnion \
          --verb FileSave \
          --verb FileClose \
          --verb FileQuit \
A.svg

